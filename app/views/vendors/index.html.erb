<p id="notice"><%= notice %></p>

<h1>Vendors</h1>

<table>
  <thead>
    <tr>
      <th>State</th>
      <th>Vendor status</th>
      <th>Vendorname</th>
      <th>Contactname</th>
      <th>Jobtitle</th>
      <th>Workphone</th>
      <th>Mobile</th>
      <th>Fax</th>
      <th>Vendoremail</th>
      <th>Vendorccemail</th>
      <th>Website</th>
      <th>Streetaddress1</th>
      <th>Streetaddress2</th>
      <th>City</th>
      <th>Zipcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vendors.each do |vendor| %>
      <tr>
        <td><%= vendor.state %></td>
        <td><%= vendor.vendor_status %></td>
        <td><%= vendor.VendorName %></td>
        <td><%= vendor.ContactName %></td>
        <td><%= vendor.JobTitle %></td>
        <td><%= vendor.WorkPhone %></td>
        <td><%= vendor.Mobile %></td>
        <td><%= vendor.Fax %></td>
        <td><%= vendor.VendorEmail %></td>
        <td><%= vendor.VendorCCEmail %></td>
        <td><%= vendor.Website %></td>
        <td><%= vendor.StreetAddress1 %></td>
        <td><%= vendor.StreetAddress2 %></td>
        <td><%= vendor.City %></td>
        <td><%= vendor.Zipcode %></td>
        <td><%= link_to 'Show', vendor %></td>
        <td><%= link_to 'Edit', edit_vendor_path(vendor) %></td>
        <td><%= link_to 'Destroy', vendor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vendor', new_vendor_path %>
