<p id="notice"><%= notice %></p>

<h1>Ship Via</h1>

<table>
  <thead>
    <tr>
      <th>Ship via description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ship_via.each do |ship_vium| %>
      <tr>
        <td><%= ship_vium.ship_via_description %></td>
        <td><%= link_to 'Show', ship_vium %></td>
        <td><%= link_to 'Edit', edit_ship_vium_path(ship_vium) %></td>
        <td><%= link_to 'Destroy', ship_vium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ship Vium', new_ship_vium_path %>
