<div class="content-wrapper">

  <div class="container-fluid">

    <!-- Breadcrumbs-->
    <%= render partial: 'partials/breadcrumbs' %>

    <!-- Import Vendor Data -->
    <div>
      <h5> Import Vendor Data: </h5>
      <%= form_tag import_vendors_path, multipart: true do %>
        <%= file_field_tag :file %>
        <%= submit_tag "Import Vendor" %>
      <% end %>
    </div>

    <!-- Vendor Table-->
    <%= render partial: 'partials/tables/vendor-table' %>
  </div>

</div>
